    <!-- <meta name="description" content= -->
    <% if yield(:meta_description).present? %>
        <meta name="description" content="<%= yield(:meta_description) %>" />
      <% else %>
        <meta name="description" content="Acens Empresa Júnior de Computação - UECE" />
      <% end %>

  	<meta name="keywords" content="Site, Web, Desenvolvimento, Desing, Acens, Empresa júnior" />
